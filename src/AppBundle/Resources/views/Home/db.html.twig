{% extends 'AppBundle::base.html.twig' %}

{% block title %}GET - {{ parent() }}{% endblock %}

{% block body %}

<h3>DB TEST</h3>
<div>
  <table class="table table-hover table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>E-MAIL</th>
        <th>TELL</th>
      </tr>
    </thead>
    <tbody>
      {% for value in inquiry %}
      <tr>
        <td>{{value.id}}</td>
        <td>{{value.name}}</td>
        <td>{{value.email}}</td>
        <td>{{value.tell}}</td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
{% endblock %}
